public int[] front11(int[] a, int[] b) {
  
  //mengecek kapasitas array yang mau dibuat
  int i=0;
  if(a.length!=0){
    i=i+1;
  }if(b.length!=0){
    i=i+1;
  }
  
  int[] res = new int[i];
  
  //untuk cek, apakah udah terisi slot depannya
  int j=0;
  if(a.length>0){
    res[j]=a[0];
    j=j+1;
  }if(b.length>0){
    res[j]=b[0];
  }
  return res;
}
