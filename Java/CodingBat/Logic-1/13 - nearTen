public boolean nearTen(int num) {
  
  int temp=0;
  
  if(temp+2==num || temp+1==num || temp-2 == num || temp-1 == num || temp==num){
    return true;
  }
  
  while(temp<num){
    temp=temp+10;
     if(temp+2==num || temp+1==num || temp-2 == num || temp-1 == num || temp==num){
    return true;
  }
  }
  return false;
  
 
  
  
  
}
