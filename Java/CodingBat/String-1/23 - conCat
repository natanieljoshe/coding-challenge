public String conCat(String a, String b) {
  
  String res="";
  int start=0;
  if(a.length()==0 || b.length()==0){
    return a+b;
  }
  
  if(a.substring(a.length()-1,a.length()).equals(b.substring(0,1))){
    start=1;
  }
  
  res = a.substring(0,a.length())+b.substring(start,b.length());
  return res;
  
}
