public String lastTwo(String str) {
  
  
  if(str.length()<=1){
    return str;
  }
  
  String last1="";
  String last2="";
  String back="";
  last1 = str.substring(str.length()-1,str.length());
  last2 = str.substring(str.length()-2,str.length()-1);
  back = last1+last2;
  
  if(str.length()==2){
    return back;
  }
  return str.substring(0,str.length()-2)+back;
  
}
