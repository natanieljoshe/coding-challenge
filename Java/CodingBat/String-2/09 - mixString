public String mixString(String a, String b) {
 
 String res="";
 int end=0;
 if(a.length()>=b.length()){
   end=a.length();
 }else{
   end=b.length();
 }
 
 for(int i=0;i<end;i++){
   if(a.length()>i){
     res=res+a.charAt(i);
   }if(b.length()>i){
     res=res+b.charAt(i);
   }
 }
 return res;
  
}
