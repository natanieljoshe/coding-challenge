public String stringYak(String str) {
  
  String res = "";
  
  for(int i=0;i<str.length();i++){
    int end = i+3;
    if(end>str.length()){
      end = str.length();
    }
    if(!str.substring(i,end).equals("yak")){
      res = res + str.substring(i,i+1);
      
    }else{
      i=i+2;
    }
  }
   return res; 
  
}

